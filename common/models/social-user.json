{
  "name": "socialUser",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "userId": {
      "type": "any",
      "required": true
    },
    "provider": {
      "type": "string",
      "required": true
    },
    "externalId": {
      "type": "string",
      "required": true
    },
    "created_at": {
      "type": "date"
    },
    "modified_at": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    },    
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "accessType": "READ",
      "permission": "ALLOW"
    }

  ],
  "methods": {}
}
